{{#if visible}}
  {{#if config.customCommitCancelComponent}}
    {{component config.customCommitCancelComponent commit=commit cancel=cancel}}
  {{else}}
    <div class="commit-cancel {{strip-class-names classNames}}">
      <button class="{{config.buttonClasses}} commit" type="button" {{action commit}}>✔</button>
      <button class="{{config.buttonClasses}} cancel" type="button" {{action cancel}}>✘</button>
    </div>
  {{/if}}
{{/if}}

